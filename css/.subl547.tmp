/*********************************************************************************************/
/*                                         header styles                                     */
/*********************************************************************************************/

header{
	position:relative;
	top:0;
	z-index:50;
	height:90px;
/*box-shadow*/
	-webkit-box-shadow:rgba(0, 0, 0, 0.1) 0 4px 4px 0;
	   -moz-box-shadow:rgba(0, 0, 0, 0.1) 0 4px 4px 0;
	        box-shadow:rgba(0, 0, 0, 0.1) 0 4px 4px 0;
/*background: */
	background:#fff;
}
.headwrap{
	padding-top:13px;
}
.headwrap > a > img{
	position:relative;
	left:8px;
}
.headwrap aside{
	display:inline-block;
	width:200px;
	position:relative;
	top:-22px;
	left: 34px;
}
.mainmenu li a, .headwrap aside a{
	color:#585951;
}
.headwrap aside a{
	font-size:.98em;
}
.headwrap aside>a:first-child::after{
	content:" | ";
}
.headwrap aside>a:nth-child(2){
	content:none;
}
nav.mainmenu{
	padding-right:40px;
	position:relative;
}
.mainmenu ul > li{
	margin:0 10px;
}
.mainmenu ul > li > a{
	font-size:0.9em;
}
.mainmenu ul > li:hover > a{
	color:#2F95C8;
	position:relative;
	z-index:100;
	padding:12px 12px 17px 12px;
	margin:-13px;
	background:#fff;
	border-top-left-radius:4px;
	border-top-right-radius:4px;
	border-top:1px solid #ccc;
	border-left:1px solid #ccc;
	border-right:1px solid #ccc;
	border-bottom:none;
/*box-shadow*/
	-webkit-box-shadow:rgba(0, 0, 0, 0.15) -6px 0px 10px -2px;
	   -moz-box-shadow:rgba(0, 0, 0, 0.15) -6px 0px 10px -2px;
	        box-shadow:rgba(0, 0, 0, 0.15) -6px 0px 10px -2px;
}
.mainmenu li:last-child{
	margin-right:0;
}
.mainmenu li:first-child{
	margin-left:0;
}
.submenu{
	display:none;
	width:940px;
	min-height:240px;
	position:absolute;
	z-index:10;
	top:34px;
	left:-180px;
	background:#fff;
	border:1px solid #ccc;
/*border-radius*/
	-webkit-border-radius:4px;
	   -moz-border-radius:4px;
	        border-radius:4px;
/*box-shadow*/
	-webkit-box-shadow:rgba(0, 0, 0, 0.25) 0 6px 6px 2px;
	   -moz-box-shadow:rgba(0, 0, 0, 0.25) 0 6px 6px 2px;
	        box-shadow:rgba(0, 0, 0, 0.25) 0 6px 6px 2px;
}
.submenu > ul{
	padding:15px;
	float:left;
	text-align:left;
}
	.submenu > ul > li{
	margin:2px 0;
	display:list-item;
	list-style:none;
	min-width:220px;
}
.submenu > ul > li > a{
	font-size:1em;
}
.submenu > ul > li > a:hover, .submenu > ul > li:hover > a{
	color:#2F95C8;
	border:none;
	margin:0;
	padding:0;
	text-decoration: underline;
/*box-shadow*/
	-webkit-box-shadow:none;
	   -moz-box-shadow:none;
	        box-shadow:none;
}
.submenu li > img{
	position:absolute;
	right:20px;
	padding-left:20px;
	border-left:1px solid #2F95C8;
}
.mainmenu a.underandblue{
	text-decoration:underline;
	color:#2F95C8;
	font-family:'robotoregular';
	font-size:0.95em;
}
.mainmenu ul{
	padding-left: 40px;
}
.mainmenu ul > li:hover > .submenu{
	display:block;
}
.searchbtn{
	position:relative;
	top:24px;
	border:1px solid #D2D2D2;
/*border-radius*/
	-webkit-border-radius:5px;
	   -moz-border-radius:5px;
	        border-radius:5px;
	height:24px;
}
.searchbtn input[type='text']{
	width:85%;
	height:85%;
	background:none;
	border:none;
	position:relative;
	top:-3px;
	border-right:1px solid #D2D2D2;
}
.searchbtn input[type='submit']{
	width:11%;
	background:url('../img/searchBtn.png') no-repeat center;
	border:none;
	text-align:right;
}
a.button{
	display:block;
	position:relative;
	font-size:0.94em;
	text-shadow:-1px 0px 3px rgba(25, 25, 25, 0.6);
	width:180px;
	padding:0.45em;
/*border-radius*/
	-webkit-border-radius:5px;
	   -moz-border-radius:5px;
	        border-radius:5px;
/**  gradient background color  **/
/*linear-gradient*/
	background:-webkit-gradient(linear,left top,left bottom,color-stop(#2F95C8,0),color-stop(#2C96CA,0.24),color-stop(#1188C4,1));
	background:-webkit-linear-gradient(#2F95C8 0%, #2C96CA 24%, #1188C4 100%);
	background:   -moz-linear-gradient(#2F95C8 0%, #2C96CA 24%, #1188C4 100%);
	background:     -o-linear-gradient(#2F95C8 0%, #2C96CA 24%, #1188C4 100%);
	background:        linear-gradient(#2F95C8 0%, #2C96CA 24%, #1188C4 100%);
/*box-shadow*/
	-webkit-box-shadow:rgba(0, 0, 0, 0.1) 0 4px 4px 0;
	   -moz-box-shadow:rgba(0, 0, 0, 0.1) 0 4px 4px 0;
	        box-shadow:rgba(0, 0, 0, 0.1) 0 4px 4px 0;
}
a.button span:before{
	font-family: 'FontAwesome';
	content: "\f054";
	width:0;
	height:0;
	position:absolute;
	right:20px;
	top:7px;
}
a.button:hover{
	text-shadow:-1px 0px 3px rgba(25, 25, 25, 0.3);
	background:linear-gradient(#3ea1d4 0%, #349ed4 24%, #289ad2 100%);
/*box-shadow*/
	-webkit-box-shadow:inset 0px 0px 2px #FFF;
	   -moz-box-shadow:inset 0px 0px 2px #FFF;
	        box-shadow:inset 0px 0px 2px #FFF;
}
a.button:hover span:before{
	background:rgba(255, 255, 255, 0.9);
	font-family: 'FontAwesome';
	content: "\f192";
	right:22px;
}
