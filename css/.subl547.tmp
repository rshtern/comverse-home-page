section.news{
	min-height:450px;
	background:url('../img/pic02.jpg') center center no-repeat;
/*background-size*/
	-webkit-background-size:/*@@prefixmycss->No equivalent*/;
	   -moz-background-size:cover;
	     -o-background-size:/*@@prefixmycss->No equivalent*/;
	        background-size:cover;
	background-attachment:fixed;
	position: relative;
	z-index: 2;
/*box-shadow*/
	-webkit-box-shadow:rgba(0, 0, 0, 0.1) 0 4px 4px 0;
	   -moz-box-shadow:rgba(0, 0, 0, 0.1) 0 4px 4px 0;
	        box-shadow:rgba(0, 0, 0, 0.1) 0 4px 4px 0;
}
.news h1{
	padding-top:54px;
	text-align:center;
}
.box-news{
	width:300px;
	min-height:120px;
	padding:10px;
	background:rgba(255, 255, 255, 0.5);
	margin-bottom:10px;
/*border-radius*/
	-webkit-border-radius:3px;
	   -moz-border-radius:3px;
	        border-radius:3px;
}
.box-news:hover{
	background:rgba(255, 255, 255, 0.75);

