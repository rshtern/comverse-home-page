.description + img{
	float: right;
	position: absolute;
	bottom: 10px;
	right: 4px;
}
.description h4{
	color: #47ACDD;
	text-shadow: none;
}
.box-large cite, .box-medium cite{
	font-family: robotolight;
	color: #6D6D6D;
	font-size: 1.2em;
	font-style: normal;
}
.description p{
	color: #6D6D6D;
}
.front{
	z-index: 100;
    opacity: 1;
	background: #fff;
}
.front, .back{
	position: absolute;
	width: inherit;
	height: inherit;
}
.back{
	z-index: 10;
    opacity: 1;
	background: #3AA6DB;
}
.back a cite{
	color: #fff;
	margin: 10px;
	display: block;
}
.back a cite i.fa-quote-right:first-child{
	float: left;
	margin-bottom: 180px;
}
/*********************************************************************************************/
/*                                        box animations                                     */
/*********************************************************************************************/
.front{   
    -webkit-transition: transform 0.8s ease-in-out;
    -moz-transition: transform 0.8s ease-in-out;
    -o-transition: transform 0.8s ease-in-out;
    -ms-transition: transform 0.8s ease-in-out;
    transition: transform 0.8s ease-in-out;
}
.front:hover{
    -webkit-transform: translateY(300px);
    -moz-transform: translateY(300px);
    -o-transform: translateY(300px);
    -ms-transform: translateY(300px);
    transform: translateY(300px);
}

