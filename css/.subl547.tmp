.date:first-child{
	padding-bottom:13px;
	border-bottom:1px dotted #fff;
	border-top-left-radius:3px;
	border-top-right-radius:3px;
}
.date:last-child{
	padding-top:0px;
	border-bottom-left-radius:3px;
	border-bottom-right-radius:3px;
}
article .datebox + p{
	padding:0 0 0 10px;
